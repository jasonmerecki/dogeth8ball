{
  "name": "dogeth8ball",
  "version": "1.0.0",
  "description": "A magic 8-ball app for Ethereum, and also, Doge meme. Much. Wow.",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "gethinitdev": "geth --datadir=./chaindata/ init ./genesis.json",
    "gethdev": "geth --datadir=./chaindata/ --rpc",
    "gethattach": "geth attach --datadir=./chaindata/ ipc:/Users/jasonmerecki/wrkspaces/dogeth8ball/chaindata/geth.ipc",
    "truffle": "truffle",
    "ganache": "ganache-cli -p 7545 ",
    "ganachelimit": "ganache-cli -p 7545 --gasLimit 5000000",
    "truffletest": "truffle test",
    "devDeploy": "truffle migrate --network development",
    "localGethDeploy": "truffle migrate --network localGeth",
    "truffleBuild": "truffle compile"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonmerecki/dogeth8ball.git"
  },
  "author": "Jason Merecki <jasonmerecki@gmail.com>",
  "private": false,
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4",
    "create-react-app": "^1.5.2",
    "eth-random": "0.0.4-alpha.1",
    "ganache-cli": "^6.1.0",
    "truffle": "4.0.1",
    "web3": "0.20.1",
    "zeppelin-solidity": "1.2.0"
  }
}
